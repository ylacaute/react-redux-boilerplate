{
  "name": "react-redux-boilerplate",
  "title": "react-redux-boilerplate",
  "version": "3.0.0",
  "description": "Webpack 3 Boilerplate with React, Redux, Babel, SASS",
  "author": "Yannick Lacaute",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ylacaute/react-redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://ylacaute@github.com/ylacaute/react-redux-boilerplate.git"
  },
  "scripts": {
    "test": "node ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "dev": "NODE_ENV=dev node_modules/.bin/webpack-dev-server --progress",
    "api": "./node_modules/.bin/json-server api/db.json --routes api/routes.json",
    "prod": "NODE_ENV=prod webpack -p --progress",
    "serve": "node_modules/.bin/http-server dist"
